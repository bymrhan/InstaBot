{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {

   },
   "description": "Re-enable the possibility to use the context menu, copy, paste, and text selection on sites that prevent them",
   "homepage_url": "https://add0n.com/allow-right-click.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "512": "data/icons/512.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Allow Right-Click",
   "optional_permissions": [ "webNavigation", "*://*/*" ],
   "options_ui": {
      "open_in_tab": false,
      "page": "data/options/index.html"
   },
   "permissions": [ "storage", "activeTab", "contextMenus", "notifications" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.5.0"
}
